{
  "Git": {
    "CommiterName": "ChangelogBot",
    "CommiterEmail": "ChangelogBot@noreply.com",
    "CommitMessageParseRegex": "^Merged PR (\\d+): (\\w*): (.*)"
  },
  "ChangelogGenerator": {
    "OutputFilePath": "CHANGELOG.md",
    "IssueUrlFormat": "https://dev.azure.com/ScholtensIO/NET-101/_workitems/edit",
    "Title": "Changelog",
    "FeatureSectionTitle": ":star: Features",
    "FixesSectionTitle": ":bug: Fixes"
  },
  "Publisher": {
    "PublishFile": "CHANGELOG.md",
    "CommitMessage": "[SKIP CI] Committed CHANGELOG.md"
  },
  "Nuget": {
    "Source": "https://api.nuget.org/v3/index.json"
  }
}